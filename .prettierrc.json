{
  "requirePragma": false,
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "none",
  "bracketSpacing": true,
  "overrides": [
    {
      "files": [
        "*.md"
      ],
      "options": {
        "proseWrap": "always"
      }
    }
  ]
}
